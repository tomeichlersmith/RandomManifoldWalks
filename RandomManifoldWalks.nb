Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", 
   RowBox[{"distance", ",", "geodist"}], "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"distance", "[", "geoeqn_", "]"}], "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"u0_", ",", "v0_"}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{"u1_", ",", "v1_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"uu", ",", "vv", ",", "u", ",", "v"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{"uu", ",", "vv"}], "}"}], "=", 
       RowBox[{"NDSolveValue", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"u", "''"}], "[", "t", "]"}], "\[Equal]", 
            RowBox[{
             RowBox[{"geoeqn", "[", 
              RowBox[{
               RowBox[{"u", "[", "t", "]"}], ",", 
               RowBox[{"v", "[", "t", "]"}], ",", 
               RowBox[{
                RowBox[{"u", "'"}], "[", "t", "]"}], ",", 
               RowBox[{
                RowBox[{"v", "'"}], "[", "t", "]"}]}], "]"}], "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{
             RowBox[{"v", "''"}], "[", "t", "]"}], "\[Equal]", 
            RowBox[{
             RowBox[{"geoeqn", "[", 
              RowBox[{
               RowBox[{"u", "[", "t", "]"}], ",", 
               RowBox[{"v", "[", "t", "]"}], ",", 
               RowBox[{
                RowBox[{"u", "'"}], "[", "t", "]"}], ",", 
               RowBox[{
                RowBox[{"v", "'"}], "[", "t", "]"}]}], "]"}], "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], ",", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"u", "[", "0", "]"}], "\[Equal]", "u0"}], ",", 
           RowBox[{
            RowBox[{"v", "[", "0", "]"}], "\[Equal]", "v0"}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"u", "[", "1", "]"}], "\[Equal]", "u1"}], ",", 
           RowBox[{
            RowBox[{"v", "[", "1", "]"}], "\[Equal]", "v1"}]}], "}"}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"u", ",", "v"}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{"{", 
          RowBox[{"t", ",", "0", ",", "1"}], "}"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"Integrate", "[", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             RowBox[{"uu", "'"}], "[", "t", "]"}], ")"}], "2"], "+", 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{
             RowBox[{"vv", "'"}], "[", "t", "]"}], ")"}], "2"]}], "]"}], ",", 
        
        RowBox[{"{", 
         RowBox[{"t", ",", "0", ",", "1"}], "}"}]}], "]"}]}]}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"geodist", "[", "geoeqn_", "]"}], "[", 
    RowBox[{"reg_", ",", 
     RowBox[{"{", 
      RowBox[{"u0_", ",", "v0_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "pt", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Minimize", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"distance", "[", "geoeqn", "]"}], "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"u0", ",", "v0"}], "}"}], ",", "pt"}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"pt", " ", "\[Element]", "reg"}]}], "]"}], "[", 
      RowBox[{"[", "1", "]"}], "]"}]}], "\[IndentingNewLine]", "]"}]}], 
  ";"}]}], "Input",
