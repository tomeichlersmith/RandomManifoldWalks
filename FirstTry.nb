Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["u",
     FontWeight->"Bold"], ",", 
    StyleBox["v",
     FontWeight->"Bold"]}], TraditionalForm]],
