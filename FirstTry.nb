Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", 
   RowBox[{"geodesicequations", ",", "nsteponce", ",", "nsteponceRANDANG"}], 
   "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"geodesicequations", "[", 
     RowBox[{"{", 
      RowBox[{"x_", ",", 
       RowBox[{"{", 
        RowBox[{"umin_", ",", "umax_"}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"vmin_", ",", "vmax_"}], "}"}]}], "}"}], "]"}], "[", 
    RowBox[{"u_", ",", "v_", ",", "p_", ",", "q_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"xu", ",", "xv", ",", "xuu", ",", "xuv", ",", "xvv"}], "}"}], 
     ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"xu", "=", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"x", "[", 
          RowBox[{"u", ",", "v"}], "]"}], ",", "u"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"xv", "=", 
       RowBox[{"D", "[", 
        RowBox[{
         RowBox[{"x", "[", 
          RowBox[{"u", ",", "v"}], "]"}], ",", "v"}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"xuu", "=", 
       RowBox[{"D", "[", 
        RowBox[{"xu", ",", "u"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"xuv", "=", 
       RowBox[{"D", "[", 
        RowBox[{"xu", ",", "v"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"xvv", "=", 
       RowBox[{"D", "[", 
        RowBox[{"xv", ",", "v"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"Simplify", "[", "\[IndentingNewLine]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "/", 
            RowBox[{"xu", ".", "xu"}]}], ")"}], "*", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"xuu", ".", "xu"}], " ", 
             SuperscriptBox["p", "2"]}], "+", 
            RowBox[{"2", 
             RowBox[{"xuv", ".", "xu"}], " ", "p", " ", "q"}], "+", 
            RowBox[{
             RowBox[{"xvv", ".", "xu"}], " ", 
             SuperscriptBox["q", "2"]}]}], ")"}]}], ",", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"-", "1"}], "/", 
            RowBox[{"xv", ".", "xv"}]}], ")"}], "*", 
          RowBox[{"(", 
