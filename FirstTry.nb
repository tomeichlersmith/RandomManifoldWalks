      RowBox[{"u_", ",", "v_"}], "]"}], ":=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"R", "+", 
          RowBox[{"r", " ", 
           RowBox[{"Cos", "[", "u", "]"}]}]}], ")"}], 
        RowBox[{"Cos", "[", "v", "]"}]}], ",", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"R", "+", 
          RowBox[{"r", " ", 
           RowBox[{"Cos", "[", "u", "]"}]}]}], ")"}], 
        RowBox[{"Sin", "[", "v", "]"}]}], ",", 
       RowBox[{"r", " ", 
        RowBox[{"Sin", "[", "u", "]"}]}]}], "}"}]}], ";", 
    "\[IndentingNewLine]", 
