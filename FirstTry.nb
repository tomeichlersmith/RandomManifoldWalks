Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["u",
     FontWeight->"Bold"], ",", 
    StyleBox["v",
     FontWeight->"Bold"]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b1dc999a-0636-49c0-8c44-36bf6cdea0cd"],
